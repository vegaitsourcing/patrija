<div class="project">
	<div class="wrap">
		<div class="project__item project__item--normal">
			<div class="project__item-content">
				<h2 class="h2 project__title">{{title}}</h2>
				<p class="project__text">{{text}}</p>
				<a class="btn" href="javascript:;">{{link}}</a>
			</div>
			<div class="project__image project__image--normal">
				{{#if additionalImage}}
				<img class="project__additional-image" src="{{additionalImage}}" alt="">
				{{/if}}
				<img src="{{image}}" alt="">
			</div>
		</div>
		<div class="project__item project__item--reverse">
			<div class="project__item-content">
				<h2 class="h2 project__title">{{titleReverse}}</h2>
				<p class="project__text">{{textReverse}}</p>
				<a class="btn" href="javascript:;">{{linkReverse}}</a>
			</div>
			<div class="project__image project__image--reverse">
				{{#if additionalImageReverse}}
				<img class="project__additional-image" src="{{additionalImageReverse}}" alt="">
				{{/if}}
				<img src="{{imageReverse}}" alt="">
			</div>
		</div>
		<span class="circle circle--blue"></span>
	</div>
</div>

