<div class="donations">
	<div class="wrap wrap--small">
		<div class="donations__content">
			<h3 class="h3 donations__title">{{title}}</h3>
			<p class="donations__text">{{text}}</p>
		</div>
		<div class="donations__logos">
			{{#each logo}}
				<div class="donations__logo">
					<img class="donations__logo-image" src="{{this.image}}" alt="">
				</div>
			{{/each}}
		</div>
	</div>
</div>
