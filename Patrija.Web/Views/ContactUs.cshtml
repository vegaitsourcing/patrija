@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContactUsViewModel>

@if (Model.MapInfo != null)
{
    Html.RenderAction<NestedContentController>(c => nameof(c.ContactMap), new { viewModel = Model.MapInfo });
}

@if (Model.Form != null)
{
    Html.RenderAction<NestedContentController>(c => nameof(c.ContactForm), new { viewModel = Model.Form });
}